(prev-language "English")
(config-version "3.0.6")
(import-add-alpha yes)
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(monitor-xresolution 157)
(monitor-yresolution 161)
(override-theme-icon-size yes)
(custom-icon-size large)
(show-welcome-dialog no)
(selection-grow-radius 4)
(selection-border-style hard)
(fill-options
    (style fg-color)
    (custom-style solid-color)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style fg-color)
    (custom-style solid-color)
    (antialias yes)
    (feather no)
    (feather-radius 10)
    (method line)
    (width 6)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))


